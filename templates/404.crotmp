<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>404 error - Raku Docs</title>
    <link rel="stylesheet" href="/css/main.css">
    <link rel="stylesheet" href="/css/themes/light.css">

    <script type="text/javascript" src="../js/core.js"></script>

  </head>
  <body>
    <section class="hero error-page is-fullheight">
      <div class="hero-body">
        <div class="container">

          <div class="error-icon">
            <img src="/img/camelia-recoloured.png">
          </div>

          <divv class="error-content">
            <h1 class="title error-title">
              404
            </h1>


            <h2 class="subtitle is-4">
              Sorry, the page you requested <strong>was not found.</strong>
            </h2>

            <h3 class="subtitle is-6">
              This error is already reported. Thank you for noticing it!
            </h3>

            <div class="columns is-vcentered">
              <div class="column is-1 is-5-mobile">
                <a href="/" class="button is-link">Home</a>
              </div>
              <div class="column is-4 is-5-mobile">

                <div class="field">
                  <div class="control has-icons-right">
                    <input class="input" type="text" placeholder="Search...">
                    <span class="icon is-small is-right">
                      <i class="fas fa-search"></i>
                    </span>
                  </div>
                </div>
              </div>
            </div>

          </div>

        </div>
      </div>
    </section>

    <!-- Include Font Awesome -->
    <script defer src="https://use.fontawesome.com/releases/v5.3.1/js/all.js"></script>
  </body>
</html>
